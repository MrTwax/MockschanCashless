<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="false">
        <domain includeSubdomains="false">cashless.local</domain>
        <trust-anchors>
            <!-- dein CA-Zertifikat in res/raw/local_ca.crt -->
            <certificates src="@raw/local_ca" />
        </trust-anchors>
    </domain-config>
</network-security-config>
